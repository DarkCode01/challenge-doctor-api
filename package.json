{
  "name": "challenge-doctor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "prepare": "husky install",
    "test:init": "jest --init",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "seed": "npx sequelize-cli db:seed",
    "seed:reset": "npx sequelize-cli db:seed:undo:all && npm run seed",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-paginate": "^1.0.2",
    "faker": "^5.5.3",
    "morgan": "~1.9.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "sequelize-cli": "^6.2.0",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "logger": {
    "module": {
      "name": "morgan",
      "arguments": [
        "dev"
      ]
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
